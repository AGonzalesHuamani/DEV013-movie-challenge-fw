{
    "version": 2,
    "builds": [
      {
        "src": "./package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "continue": true
      },  
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }